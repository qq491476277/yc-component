{
  "name": "mynuxt",
  "version": "1.0.0",
  "description": "My majestic Nuxt.js project",
  "author": "zengyongchao",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --fix --ignore-path .gitignore .",
    "precommit": "yarn lint",
    "test": "jest",
    "dev": "cross-env NODE_ENV=development nodemon --inspect server/index.js --watch server",
    "build": "nuxt build",
    "build:dev": "cross-env NODE_ENV=dev nuxt build && cross-env NODE_ENV=dev node .",
    "build:qa": "cross-env NODE_ENV=qa nuxt build  && cross-env NODE_ENV=qa node .",
    "build:prod": "cross-env NODE_ENV=prod nuxt build  && cross-env NODE_ENV=prod node .",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@nuxtjs/style-resources": "^1.0.0",
    "ali-oss": "^5.3.1",
    "axios": "^0.19.0",
    "classlist-polyfill": "^1.2.0",
    "co": "^4.6.0",
    "cross-env": "5.2.0",
    "js-cookie": "2.2.1",
    "koa": "^2.6.2",
    "nuxt": "^2.0.0",
    "vue-awesome-swiper": "^3.1.3",
    "vue-lazyload": "^1.3.3",
    "vue2-datepicker": "3.0.2"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.4.0",
    "@babel/traverse": "^7.6.3",
    "@nuxtjs/eslint-config": "^0.0.1",
    "@nuxtjs/eslint-module": "^0.0.1",
    "@vue/test-utils": "^1.0.0-beta.27",
    "autoprefixer": "^9.6.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": ">=22.3.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^5.2.2",
    "jest": "^24.1.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "nodemon": "^1.18.9",
    "vue-jest": "^3.0.3"
  }
}
